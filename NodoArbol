
    int dato;
    NodoArbol hijoizq, hijoderecho;
    String nombre;
    
    public NodoArbol(int d, String nom){
        this.dato=d;
        this.nombre= nom;
        this.hijoizq= null;
        this.hijoderecho= null;
    }
    
    @Override
    public String toString() {
        return "NodoArbol{" + "dato=" + dato + ", nombre=" + nombre + '}';
    }
